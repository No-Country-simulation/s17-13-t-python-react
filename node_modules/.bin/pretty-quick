#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/wanderlee/Desktop/s17-13-t-python-react/node_modules/.pnpm/pretty-quick@4.0.0_prettier@3.3.3/node_modules/pretty-quick/lib/node_modules:/home/wanderlee/Desktop/s17-13-t-python-react/node_modules/.pnpm/pretty-quick@4.0.0_prettier@3.3.3/node_modules/pretty-quick/node_modules:/home/wanderlee/Desktop/s17-13-t-python-react/node_modules/.pnpm/pretty-quick@4.0.0_prettier@3.3.3/node_modules:/home/wanderlee/Desktop/s17-13-t-python-react/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/wanderlee/Desktop/s17-13-t-python-react/node_modules/.pnpm/pretty-quick@4.0.0_prettier@3.3.3/node_modules/pretty-quick/lib/node_modules:/home/wanderlee/Desktop/s17-13-t-python-react/node_modules/.pnpm/pretty-quick@4.0.0_prettier@3.3.3/node_modules/pretty-quick/node_modules:/home/wanderlee/Desktop/s17-13-t-python-react/node_modules/.pnpm/pretty-quick@4.0.0_prettier@3.3.3/node_modules:/home/wanderlee/Desktop/s17-13-t-python-react/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pretty-quick/lib/cli.mjs" "$@"
else
  exec node  "$basedir/../pretty-quick/lib/cli.mjs" "$@"
fi
