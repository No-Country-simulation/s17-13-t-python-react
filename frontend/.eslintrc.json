{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "singleQuote": true
      }
    ],
    "@next/next/no-img-element": "off",
    "react-hooks/exhaustive-deps": "off",
    "quotes": ["error", "single"]
  }
}
